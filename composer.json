{
    "require": {
        "filp/whoops": "^2.7",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "league/container": "^3.3",
        "league/event": "^2.2",
        "league/plates": "^3.3",
        "league/route": "^4.5"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.5",
        "pestphp/pest": "^0.2.2",
        "phpstan/phpstan": "^0.12.31",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse src tests --level 6",
        "test": "./vendor/bin/pest"
    }
}
